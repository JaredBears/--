    var secondLevelMenuOpenOnMobileClassName = 'second-level-open-on-mobile';
    document.querySelectorAll('.sidebar-holder .sidebar-mobile .hasdrop').forEach(function (element) {
        element.addEventListener('click', function (event) {
            // Prevents hiding the menu when clicking the anchor in submenu
            if (!event.target.href) {
                event.currentTarget.classList.add('open');
                event.currentTarget.classList.contains('open') && document.body.classList.add(secondLevelMenuOpenOnMobileClassName);
            }
        });
    })
</script>
