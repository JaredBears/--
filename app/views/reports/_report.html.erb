<div id="<%= dom_id report %>">
  <p>
    <strong>Lat:</strong>
    <%= report.lat %>
  </p>

  <p>
    <strong>Lng:</strong>
    <%= report.lng %>
  </p>

  <p>
    <strong>String:</strong>
    <%= report.string %>
  </p>

  <p>
    <strong>Reporter:</strong>
    <%= report.reporter_id %>
  </p>

  <p>
    <strong>Address street:</strong>
    <%= report.address_street %>
  </p>

  <p>
    <strong>Address zip:</strong>
    <%= report.address_zip %>
  </p>

  <p>
    <strong>Blu:</strong>
    <%= report.blu_id %>
  </p>

  <p>
    <strong>Complete blu:</strong>
    <%= report.complete_blu %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= report.description %>
